{
  "compilerOptions": {
    "target": "es2015",
    "jsx": "react",
    "baseUrl": "src",
    "noEmit": true,
    "moduleResolution": "node",
    "paths": {
      "src/*": ["./*"],
    },
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules",
  ],
  "extends": "@tsconfig/react-native/tsconfig.json"
}
